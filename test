wget -O /usr/bin/badvpn-udpgw "https://www.dropbox.com/s/g3ggj96itbqybd6/badvpn-udpgw?dl=0"
chmod +x /usr/bin/badvpn-udpgw
sed -i '$ i\screen -dmS badvpn badvpn-udpgw --listen-addr 127.0.0.1:7300 --max-clients 4000' --max-clients 4000 --max-connections-for-client 4000 --loglevel info /etc/rc.local
Restart=always
